{
    "name": "ddr/laravel-commands",
    "description": "Generator commands for Laravel applications",
    "keywords": [
        "danie1net0",
        "laravel",
        "laravel-commands"
    ],
    "homepage": "https://github.com/danie1net0/laravel-commands",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Neto",
            "email": "daniel@danielneto.dev.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/console": "^9.0|^10.1",
        "illuminate/support": "9.0|^10.1",
        "illuminate/validation": "9.0|^10.1",
        "laravel/helpers": "^1.6"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "laravel/pint": "^1.2",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^v2.4.0",
        "pestphp/pest-plugin-laravel": "^v2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Ddr\\LaravelCommands\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ddr\\LaravelCommands\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": "Ddr\\LaravelCommands\\Providers\\LaravelCommandsServiceProvider"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
